<?xml version='1.0' encoding='UTF-8'?>
<com.qubell.jenkinsci.plugins.qubell.Configuration plugin="qubell@<%= @version %>">
  <url><%= @endpoint %></url>
  <login><%= @username %></login>
  <password><%= @password %></password>
  <statusPollingInterval>20</statusPollingInterval>
  <skipCertificateChecks>false</skipCertificateChecks>
  <enableMessageLogging>false</enableMessageLogging>
</com.qubell.jenkinsci.plugins.qubell.Configuration>
