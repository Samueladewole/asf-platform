<?xml version='1.0' encoding='UTF-8'?>
<hudson.plugins.jira.JiraProjectProperty_-DescriptorImpl plugin="jira@1.39">
  <sites>
    <hudson.plugins.jira.JiraSite>
      <url>http://<%= @jira_host %></url>
      <alternativeUrl>https://<%= @jira_host %></alternativeUrl>
      <useHTTPAuth>true</useHTTPAuth>
      <userName><%= @jira_username %></userName>
      <password><%= @jira_password %></password>
      <supportsWikiStyleComment>true</supportsWikiStyleComment>
      <recordScmChanges>false</recordScmChanges>
      <updateJiraIssueForAllStatus>true</updateJiraIssueForAllStatus>
    </hudson.plugins.jira.JiraSite>
  </sites>
</hudson.plugins.jira.JiraProjectProperty_-DescriptorImpl>
