<VirtualHost *:*>
    ProxyPreserveHost On

    ProxyPass /grid/console http://<%= @params[:selenium_ip] %>:4444/grid/console
    ProxyPassReverse /grid/console http://<%= @params[:selenium_ip] %>:4444/grid/console

    ProxyPass /grid/resources http://<%= @params[:selenium_ip] %>:4444/grid/resources
    ProxyPassReverse /grid/resources http://<%= @params[:selenium_ip] %>:4444/grid/resources
</VirtualHost>
